{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": true }
    ],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/no-unresolved": 0
  },
  "globals": {
    "chrome": true
  }
}