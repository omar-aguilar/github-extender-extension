{
  "manifest_version": 2,
  "name": "Github Extender Extension",
  "description": "",
  "version": "",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://github.com/"
  ]
}
